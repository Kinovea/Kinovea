<?xml version="1.0" encoding="utf-8"?>
<KinoveaPostureTool>
  <FormatVersion>1.0</FormatVersion>
  <Name>Alignment Angle</Name>
  <PointCount>4</PointCount>
  <Segments>
    <Segment point1="0" point2="1" name="" style="Solid" width="1"/>
    <Segment point1="1" point2="2" name="" style="Solid" width="1"/>
    <Segment point1="1" point2="3" name="" style="Dash" width="1"/>
  </Segments>
  <Angles>
    <Angle origin="1" leg1="2" leg2="3"/>
  </Angles>
  <Handles>
    <Handle type="Point" reference="0">
      <Impact type="Align">
        <Align pointToAlign="1" AlignWith="2"/>
      </Impact>
    </Handle>
    <Handle type="Point" reference="1">
      <Constraint type="LineSlide">
        <LineSlide point1="0" point2="2" position="Inbetween"/>
      </Constraint>
    </Handle>
    <Handle type="Point" reference="2">
      <Impact type="Align">
        <Align pointToAlign="1" AlignWith="0"/>
      </Impact>
    </Handle>
    <Handle type="Point" reference="3"/>
  </Handles>
  <HitZone>
    <Polygon points="0;1;2;3"/>
  </HitZone>
  <InitialConfiguration>
    <Point>200;200</Point>
    <Point>250;200</Point>
    <Point>300;200</Point>
    <Point>300;175</Point>
  </InitialConfiguration>
</KinoveaPostureTool>