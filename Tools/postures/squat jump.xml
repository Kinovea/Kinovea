<?xml version="1.0" encoding="utf-8"?>
<KinoveaPostureTool>
  <FormatVersion>1.0</FormatVersion>
  <Name>Squat Jump</Name>
  <PointCount>6</PointCount>
  <Segments>
    <Segment point1="0" point2="1" name="" style="Solid" width="2"/>
    <Segment point1="1" point2="2" name="" style="Solid" width="2"/>
    <Segment point1="2" point2="3" name="" style="Solid" width="2"/>
    <Segment point1="3" point2="4" name="" style="Solid" width="2"/>
    <Segment point1="4" point2="5" name="" style="Solid" width="2"/>
    <Segment point1="3" point2="5" name="" style="Solid" width="2"/>
  </Segments>
  <Handles>
    <Handle type="Point" reference="0">
      <Constraint type="DistanceToPoint">
        <DistanceToPoint point="1"/>
      </Constraint>
    </Handle>
    <Handle type="Point" reference="1">
      <Constraint type="DistanceToPoint">
        <DistanceToPoint point="0"/>
      </Constraint>
      <Impact type="Pivot">
        <Pivot pivot="0" impacted="2;3;4;5"/>
      </Impact>
    </Handle>
    <Handle type="Point" reference="2">
      <Constraint type="DistanceToPoint">
        <DistanceToPoint point="1"/>
      </Constraint>
      <Impact type="Pivot">
        <Pivot pivot="1" impacted="3;4;5"/>
      </Impact>
    </Handle>
    <Handle type="Point" reference="3">
      <Constraint type="DistanceToPoint">
        <DistanceToPoint point="2"/>
      </Constraint>
      <Impact type="Pivot">
        <Pivot pivot="2" impacted="4;5"/>
      </Impact>
    </Handle>
    <Handle type="Point" reference="4">
      <Constraint type="DistanceToPoint">
        <DistanceToPoint point="3"/>
      </Constraint>
      <Impact type="Pivot">
        <Pivot pivot="3" impacted="5"/>
      </Impact>
    </Handle>
  </Handles>
  <InitialConfiguration>
    <Point>200;200</Point>
    <Point>180;300</Point>
    <Point>250;350</Point>
    <Point>200;400</Point>
    <Point>220;410</Point>
    <Point>195;410</Point>
  </InitialConfiguration>
</KinoveaPostureTool>